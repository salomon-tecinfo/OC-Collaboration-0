msgid "Add New Tenant"
msgstr "Agregar Nuevo Cliente"

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "Name"
msgstr "Nombre"

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "The name of the tenant as it will be displayed in the admin."
msgstr "El nombre del sitio inquilino que debe mostrarse en el panel de administración."

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "Host"
msgstr "Host"

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "Example: If host is \"orchardproject.net\", the tenant site URL is \"http://orchardproject.net/\""
msgstr "Ejemplo: Si el host es \"orchardproject.net\", la URL del sitio del cliente es \"http://orchardproject.net/\""

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "You may define multiple domains using the comma (,) as a separator. Use the '*.' prefix to map all subdomains."
msgstr "Debes definir múltiples dominios utilizando la coma (,) como separador. Usa el prefijo ' *.' para asignar todos los subdominios."

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "URL prefix"
msgstr "Prefijo URL"

#: "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgctxt "~/Modules/Orchard.MultiTenancy/Views/Admin/Add.cshtml"
msgid "(Optional) Example: If prefix is \"site1\", the tenant URL prefix is \"http://orchardproject.net/site1\""
msgstr "(Opcional) Ejemplo: si el prefijo es \"site1\", el prefijo alquilado del sitio es \"http://orchardproject.net/site1\""

